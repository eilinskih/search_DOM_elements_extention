{
    "manifest_version": 2,
    "name": "DOM_elements_gid",
    "version": "1.0.0",
    "web_accessible_resources": ["close.svg"],
    "action": {
      "default_icon": {
      "128": "./images/get_started128.png",
      "48": "./images/get_started32.png",
      "16": "./images/get_started16.png"
      }
    },
    "icons": {
      "128": "./images/get_started128.png",
      "48": "./images/get_started32.png",
      "16": "./images/get_started16.png"
    },
    "content_scripts": [
        {
          "matches": [
            "<all_urls>"
          ],
          "js": [
            "content.js",
          "styles.js"
        ]
        }
      ]
  }
